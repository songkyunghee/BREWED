<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.ssafy.cafe.model.dao.AdminDao">
	<select id="getOrderList" parameterType="int" resultType="map">
		select * from t_order
		where s_id = #{s_id};
	</select>
	<select id="getDoneOrderList" parameterType="int" resultType="map">
		select * from t_order
		where s_id = #{s_id} and completed = "Y";
	</select>
	
	<select id="getNotDoneOrderList" parameterType="int" resultType="map">
		select * from t_order
		where s_id = #{s_id} and completed = "N";
	</select>
</mapper>